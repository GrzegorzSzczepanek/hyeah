{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 python3 -m pip install -r ../Backend/requirements.txt && python3 -m flask --app ../Backend/src/index run -p 5000",
    "dev": "next dev --turbo",
    "dev-both": "concurrently \"npm run dev\" \"npm run flask-dev\"",
    "build": "next build && python3 -m pip install -r ../Backend/requirements.txt",
    "start": "concurrently \"next start\" \"npm run flask-dev\"",
    "dev-both": "",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@types/node": "22.7.4",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.3.0",
    "@types/socket.io-client": "^3.0.0",
    "autoprefixer": "10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "9.11.1",
    "eslint-config-next": "14.2.13",
    "i18next": "^23.15.1",
    "next": "14.2.13",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "18.3.1",
    "react-i18next": "^15.0.2",
    "react-markdown": "^9.0.1",
    "react-use": "^17.5.1",
    "socket.io-client": "^4.8.0",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2"
  }
